.centered{
	margin: auto;
}
body {
    background : #99CCFF;
}

div {
	width: 30%;
	border: 2px solid black;
	border-radius: 25px;
	margin: auto;
	background-color: #7588b9;
	min-width: 200px;
}

label {
	display: block;
	text-align: center;
	line-height: 125%;
	font-size: 1.1em;
}

input {
	display: block;
	margin: auto;
}

h2 {
	text-align: center;
	#font-family: Impact;
}

a {
	display: block;
	text-align: center;
	text-decoration: none;
}

select {
	display: block;
	margin: auto;
}

