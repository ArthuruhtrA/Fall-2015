.button {
	display: block;
	width: 5%;
	margin: auto;
}

a {
	text-decoration: none;
	color: black;
	font-size: 2em;
}

body {
	background: #99CCFF;
}

td {
	height: 10%;
	text-align: center;
	border: 1px solid black;
	border-radius: 5px;
}

table {
	display: inline;
	text-align: center;
	border: 2px solid black;
	background-color: #7588b9;
}

div {
	margin: auto;
	text-align: center;
}
